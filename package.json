{
  "name": "@ldesign/api",
  "version": "0.1.0",
  "description": "一个功能强大的通用系统接口管理包，支持插件化架构、内置系统接口、性能优化，完美集成 Vue 3",
  "keywords": [
    "ldesign",
    "vue3",
    "typescript",
    "api",
    "system-api",
    "plugin",
    "http-client",
    "cache",
    "performance",
    "interceptor"
  ],
  "author": "LDesign Team",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/api#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/api"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./plugins/*": {
      "types": "./es/plugins/*.d.ts",
      "import": "./es/plugins/*.js",
      "require": "./lib/plugins/*.cjs"
    },
    "./react": {
      "types": "./es/react/index.d.ts",
      "import": "./es/react/index.js",
      "require": "./lib/react/index.cjs"
    },
    "./react/*": {
      "types": "./es/react/*.d.ts",
      "import": "./es/react/*.js",
      "require": "./lib/react/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    },
    "./vue": {
      "types": "./es/vue/index.d.ts",
      "import": "./es/vue/index.js",
      "require": "./lib/vue/index.cjs"
    },
    "./vue/*": {
      "types": "./es/vue/*.d.ts",
      "import": "./es/vue/*.js",
      "require": "./lib/vue/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "dev": "rollup -c -w",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "lint:check": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "bench:run": "vitest bench --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "clean": "rimraf esm cjs dist types lib coverage .nyc_output",
    "size-check": "size-limit",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "bench:optimization": "vitest bench __tests__/benchmark/optimization.bench.ts",
    "performance:report": "tsx scripts/performance-monitor.ts",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test:run",
    "deploy": "tsx ../../tools/deploy/package-deployer.ts api",
    "deploy:beta": "tsx ../../tools/deploy/package-deployer.ts api --tag beta",
    "deploy:alpha": "tsx ../../tools/deploy/package-deployer.ts api --tag alpha",
    "deploy:dry-run": "tsx ../../tools/deploy/package-deployer.ts api --dry-run",
    "build:dev": "node scripts/build.js --dev",
    "generate:registry": "tsx scripts/generate-typed-registry.ts typed-registry.config.json",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ldesign/http": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@eslint-react/eslint-plugin": "^2.2.2",
    "@ldesign/builder": "workspace:*",
    "@playwright/test": "^1.40.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.47",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.18.0",
    "glob": "^10.3.10",
    "jsdom": "^24.0.0",
    "playwright": "^1.40.1",
    "react": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vitepress": "^1.0.0",
    "vitest": "^2.0.0",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "50 KB"
    }
  ],
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  }
}